{
  "name": "public-interface",
  "version": "1.0.0",
  "scripts": {
    "pa11y": "pa11y --runner axe --runner htmlcs",
    "pa11yci": "mkdir -p .pa11yci/screencaptures && pa11y-ci --config .pa11yci/pa11yci.json",
    "publish": "gh-pages -d dist",
    "assemble-gltfloader1": "echo Assembling glTF v.1 loader ... && concat -o app/scripts/ThreeDViewer/GLTFLoader1.js app/scripts/ThreeDViewer/GLTFLoader1-prepend.js node_modules/three-v86/examples/js/loaders/GLTFLoader.js app/scripts/ThreeDViewer/GLTFLoader1-append.js && echo done.",
    "watch-threedviewer": "run-s assemble-gltfloader1 parcel-watch --silent",
    "build-threedviewer": "run-s assemble-gltfloader1 parcel-build --silent",
    "parcel-watch": "parcel watch app/scripts/ThreeDViewer/main.js --out-dir .tmp/scripts/ThreeDViewer --no-cache --public-url .",
    "parcel-build": "parcel build app/scripts/ThreeDViewer/main.js --out-dir dist/scripts/ThreeDViewer --no-cache --no-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ucldc/public_interface.git"
  },
  "devDependencies": {
    "apache-server-configs": "^3.0.0",
    "autoprefixer": "^9.1.0",
    "backstopjs": "^3.5.10",
    "browser-sync": "^2.24.6",
    "concat": "^1.0.3",
    "del": "^3.0.0",
    "docco": "^0.8.0",
    "gh-pages": "^2.2.0",
    "gulp": "4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-cli": "^2.0.1",
    "gulp-file-include": "^2.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-jshint": "^2.1.0",
    "gulp-modernizr": "^3.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-useref": "^3.1.5",
    "imagemin-gifsicle": "^7.0.0",
    "jshint": "^2.9.6",
    "jshint-stylish": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.3.0",
    "pa11y-ci": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "postcss-import": "^12.0.0",
    "postcss-svg": "^2.4.0",
    "source-sans-pro": "^3.6.0"
  },
  "engines": {
    "node": ">=10.8.0"
  },
  "dependencies": {
    "backbone": "~1.2.1",
    "bootstrap-sass": "~3.4.1",
    "camera-controls": "^1.22.1",
    "font-awesome": "~4.7.0",
    "hold-event": "0.0.1",
    "imagesloaded": "~3.1.8",
    "infinite-scroll": "^3.0.5",
    "isotope-layout": "~3.0.6",
    "jquery": "^3.4.1",
    "jquery-pjax": "^2.0.1",
    "jquery.dotdotdot": "^1.7.4",
    "mediaelement": "~4.1.2",
    "nprogress": "~0.2.0",
    "openseadragon": "^2.1.0",
    "picturefill": "~2.3.1",
    "slick-carousel": "=1.8.1",
    "three": "^0.117.1",
    "three-v86": "npm:three@0.86.0",
    "typeahead.js": "~0.11.1",
    "underscore": "~1.7.0"
  },
  "browserslist": [
    "defaults"
  ]
}
